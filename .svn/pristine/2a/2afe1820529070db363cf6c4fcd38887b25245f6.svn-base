html,body,ul,ol,li,dl,dd,dt,p,h1,h2,h3,h4,h5,h,form,img,input{margin:0;padding:0;}
ul,ol{list-style:none;}
body{color:#333;font:12px/20px "宋体",Arial;}
a{text-decoration:none;color:#b42025}
a:hover{text-decoration: underline;}
img,input,button{border:0;}
.clearfix:after{
    content: ".";clear:both;display:block;height: 0;overflow: hidden;visibility: hidden;
    font-size: 1px;
}

.nav-main{margin-left:8%;margin-right: 6.28%; }
h1{font-size:40px;color:#b81e1f;padding-top:30px;padding-bottom:14px;}
.nav-left{float: left;padding-left: 20.5%;margin-top:140px;}
.nav-right{float:right;padding-right: 13.4%;margin-top:65px;}
.headline{width: 500px;height:50px;}
h2{font-size:28px;color:#979797;padding-bottom:24px;float: left;}
.pan{font-size:18px;color:#979797;padding-bottom:30px;float: right;}

.btn{width: 250px;height: 44px;background: #f5f5f5;padding:0px;color: #656565;}
.btnColor{background: #b42025;color:#fff;font-weight:bold; }
.dis{display: block;}
.disp{display: none;}
.uname{margin-top:35px;}
br{height: 0px;}
.uname,.pwd,.sub,.sub1{width: 450px;height: 56px;border: #cbcbcb 1px solid;margin-left:25px;padding-left: 12px;}
.sub,.sub1{background: #b42025;margin-bottom:52px;color: #fff;font-size: 24px;margin-top: 7px;}
.sub:hover,.sub1:hover{font-weight: bold;}
.pwd2{float: left;width: 250px;height: 56px;padding-left: 12px;}
.common{ float: right;}
.unfreeze{font-size: 18px;font-family: "微软雅黑"; visibility: hidden; margin-left:25px; }
.findpwd{font-weight:bold;font-size: 18px;font-family: "微软雅黑";visibility: visible;padding-left: 310px;}
.morenb{border: #cbcbcb 1px solid;height: 58px;margin-left:25px;margin-right: 20px; display: none;}
form{border:#cbcbcb 1px solid; }
.errspan2,.errspan1,.errspan4,.errspan5,.errspan6{visibility: hidden;border: #cc9998 1px solid;width: 415px;display: block; margin-left:85px;
    font-size: 16px;color: #a10000;background: #fff6f7;}
