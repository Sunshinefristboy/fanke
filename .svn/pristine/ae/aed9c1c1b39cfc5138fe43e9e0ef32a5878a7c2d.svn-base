html,body,ul,ol,li,dl,dd,dt,p,h1,h2,h3,h4,h5,h,form,img,input{margin:0;padding:0;}
ul,ol{list-style:none;}
body{color:#333;font:12px/20px "微软雅黑",Arial;}
a{text-decoration:none;color:#b42025}
a:hover{text-decoration: underline;}
img,input,button{border:0;}
.clearfix:after{
    content: ".";clear:both;display:block;height: 0;overflow: hidden;visibility: hidden;
    font-size: 1px;
}
/* head */
.nav-main{margin-left:8%;margin-right: 6.28%; }
h1{font-size:40px;color:#b81e1f;padding-top:30px;padding-bottom:14px;}
/* img */
.nav-left{float: left;padding-left: 20.5%;margin-top:140px;}
/* rightside */
.nav-right{float:right;padding-right: 13.4%;margin-top:65px;}
.headline{width: 500px;border-bottom:3px solid #b81e20;}
h2{font-size:28px;color:#a2a2a2;padding-bottom:12px;float: left;}
.pan{font-size:22px;color:#a2a2a2;float: right;}
/* rightside head end */
.pwd1{width: 280px;height: 56px;margin-left:25px;border: #cbcbcb 1px solid;padding-left: 12px;margin-top: 56px;}
#yz{font-size: 34px;color: #1b6731;border: #cbcbcb 1px solid;padding-top: 10px; height: 46px;margin-top: 56px;float: right;}
.uname1{width: 280px;height: 56px;border: #cbcbcb 1px solid;margin-left:25px;padding-left: 12px;}
.dxyzm{display: block;width: 160px;height: 36px;border:  #cbcbcb 1px solid;background: #f5f5f5;float: right;
   text-align: center;padding-top:20px;font-weight: bold;font-size: 16px;}
.uname,.pwd,.sub,.sub1,.pwd2{width: 465px;height: 56px;border: #cbcbcb 1px solid;margin-left:25px;padding-left: 12px;}
.checkserve{margin-top: 5px;margin-bottom: 15px;margin-left:25px;}
.sub{background: #9d9d9d;margin-bottom:52px;color: #fff;font-size: 24px;margin-top: 7px;padding-left: 0px;width: 478px;
    display: block;text-align: center;padding-top: 16px;height: 40px;}
.errspan2,.errspan1,.errspan4,.errspan5,.errspan3{visibility: hidden;border: #cc9998 1px solid;display: block; margin-left:25px;
    font-size: 16px;color: #a10000;background: #fff6f7;}
/* foot */
.coop{padding:27px 13px;}
.coop a{display:inline-block; padding-left: 26px;}
.main{margin-top:25px;}
